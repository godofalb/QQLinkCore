  </des>
  <!--以下4个信息原则上交由用户配置，可以自行修改作为用户初始配置-->
  <add key="NeedF" value="Need">
  </add>
  <add key="NeedG" value="Need">
  </add>
  <add key="NeedD" value="Need">
  </add>
  <add key="Used" value="Yes">
  </add>
  <UserSetting>
    <!--自定义需求数据，将会在插件装载时加载，可以在字符字典userArg中访问。数据不要过大，建议仅保存配置数据-->
